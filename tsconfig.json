{
  "exclude": [
    "video",
    "vendor",
    "canisters",
    "scripts",
    "tests"
  ],
  "include": [
    "src"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "ES2018",
      "DOM"
    ],
    "module": "commonjs",
    "strict": true,
    "target": "es2018",
  }
}